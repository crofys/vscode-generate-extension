  {
    key: '3',
    name: 'Joe Black',
    age: 32,
    address: 'Sidney No. 1 Lake Park',
    tags: ['cool', 'teacher'],
  },
];
  return (
    <div className="<%- name %>">
      <Table columns={columns} dataSource={data} />
    </div>
  );
}
export default <%- CameName %>