{
  "name": "vscode-generate-react-extension",
  "displayName": "vscode-generate-react-extension",
  "description": "自动创建模版文件",
  "version": "0.2.3",
  "publisher":"crofys",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-generate-react-extension.generate-react",
    "onCommand:vscode-generate-react-extension.generate-api",
    "onCommand:vscode-generate-nest-extension.generate-module"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-generate-react-extension.generate-react",
        "title": "[GZ] Generate File React"
      },
      {
        "command": "vscode-generate-react-extension.generate-api",
        "title": "[GZ] Generate Module Api"
      },
      {
        "command": "vscode-generate-nest-extension.generate-module",
        "title": "[GZ] Generate Module Nest"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vscode-generate-react-extension.generate-react"
        },
        {
          "command": "vscode-generate-react-extension.generate-api"
        },
        {
          "command": "vscode-generate-nest-extension.generate-module"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "build": "vsce package",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.3",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "consola": "^2.12.1",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "ejs": "^3.1.2"
  }
}
